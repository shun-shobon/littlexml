{
	"name": "@shun-shobon/littlexml",
	"version": "0.6.4",
	"type": "module",
	"description": "1kB XML library for Node.js, Bun, Deno, Browser, Edge runtime.",
	"repository": {
		"type": "git",
		"url": "https://github.com/shun-shobon/littlexml.git"
	},
	"homepage": "https://github.com/shun-shobon/littlexml#readme",
	"author": "NISHIZAWA Shuntaro <me@s2n.tech> (https://s2n.tech/)",
	"license": "MIT",
	"packageManager": "pnpm@10.28.1",
	"main": "./dist/index.cjs",
	"types": "./dist/index.d.cts",
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		}
	},
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"keywords": [
		"xml",
		"builder",
		"utility"
	],
	"scripts": {
		"build": "tsdown",
		"format": "prettier -w .",
		"format:check": "prettier -c .",
		"lint": "eslint .",
		"lint:fix": "eslint --fix .",
		"release": "pnpm build && changeset publish",
		"test": "vitest run",
		"test:ci": "vitest run --coverage",
		"test:watch": "vitest",
		"version-package": "changeset version && pnpm format"
	},
	"devDependencies": {
		"@changesets/cli": "2.29.8",
		"@shun-shobon/style-guide": "2.2.4",
		"@vitest/coverage-v8": "4.0.18",
		"eslint": "9.39.2",
		"prettier": "3.8.1",
		"tsdown": "0.20.1",
		"typescript": "5.9.3",
		"vitest": "4.0.18"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org"
	},
	"sideEffects": false
}
